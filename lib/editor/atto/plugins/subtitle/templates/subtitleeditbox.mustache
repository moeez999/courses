<div id='poodllsubtitle_editor' class="'poodllsubtitle_editor' subtitleset_block subtitleset_block_open" style='display: none;'>
    <div class="numb_song"></div>
    <div class="subtitleset_time">
        <div class="block_input">
            <input type="text" name="poodllsubtitle_edstart" id="poodllsubtitle_edstart" class="poodllsubtitle_edstart">
            <div class="input_arr_block">
                <div id="poodllsubtitle_startbumpup" class="input_arr input_arr_top"></div>
                <div id="poodllsubtitle_startbumpdown" class="input_arr input_arr_bot"></div>
            </div>
        </div>
        <button type="button" id="poodllsubtitle_startsetnow" class="poodllsubtitle_startsetnow now_btn">{{#str}}now, atto_subtitle{{/str}}</button>
        <div class="block_input">
            <input type="text" name="poodllsubtitle_edend" id="poodllsubtitle_edend" class="poodllsubtitle_edpart">
            <div class="input_arr_block">
                <div id="poodllsubtitle_endbumpup" class="input_arr input_arr_top"></div>
                <div id="poodllsubtitle_endbumpdown" class="input_arr input_arr_bot"></div>
            </div>
        </div>
        <button type="button" id="poodllsubtitle_endsetnow" class="poodllsubtitle_endsetnow now_btn">{{#str}}now, atto_subtitle{{/str}}</button>
    </div>
    <div class="subtitleset_text">
        <textarea class="textarea" name="poodllsubtitle_edpart" id="poodllsubtitle_edpart" class="poodllsubtitle_edpart"></textarea>
    </div>
    <div class="subtitleset_btns">
        <div class="subs_btn_block subs_basket poodllsubtitle_eddelete" id="poodllsubtitle_eddelete">
            <img src="{{imgpath}}btn_ic_1.svg">
        </div>
        <div class="subs_btn_block subs_btn_menu poodllsubtitle_edapply" id="poodllsubtitle_edapply"></div>
        <div class="subs_btn_block poodllsubtitle_edmergeup" id="poodllsubtitle_edmergeup">
            <img src="{{imgpath}}btn_ic_2.svg">
        </div>
        <div class="subs_btn_block poodllsubtitle_edsplit" id="poodllsubtitle_edsplit">
            <img src="{{imgpath}}btn_ic_3.svg">
        </div>
        <button type="button" id="poodllsubtitle_edcancel" class="poodllsubtitle_edcancel">{{#str}}cancel, atto_subtitle{{/str}}</button>
    </div>
</div>