function parseVideoURL(url) {

        var retVal = {};
        var matches;
        var success = false;

        if ( url.match('http(s)?://(www.)?youtube|youtu\.be') ) {
          if (url.match('embed')) { retVal.id = url.split(/embed\//)[1].split('"')[0]; }
          else if (url.match('shorts')) { retVal.id = url.split(/shorts\//)[1].split('"')[0]; }
            else { retVal.id = url.split(/v\/|v=|youtu\.be\//)[1].split(/[?&]/)[0]; }
            retVal.provider = "youtube";
            retVal.params = "?rel=0&start=" + @@start@@;
            retVal.videoURL = '//www.youtube.com/embed/' + retVal.id;
            retVal.success = true;
        } else if (matches = url.match(/vimeo.com\/(\d+)/)) {
            retVal.provider = "vimeo";
            retVal.params = "?h=874d9d7ec4&color=F5975F&title=0&byline=0&portrait=0#t=" + @@start@@ + "s";
            retVal.id = matches[1];
            retVal.videoURL = '//player.vimeo.com/video/' + retVal.id;
            retVal.success = true;
        }
        return retVal;
}

