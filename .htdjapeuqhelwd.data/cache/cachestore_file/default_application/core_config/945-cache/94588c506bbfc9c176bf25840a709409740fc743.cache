   var theplayer = $('#' + @@AUTOID@@ + '_audioplayer');
   var voice = fetch_voice(@@language@@,@@speaker@@);
   fetch_polly_url(usetext,voice,function(audiourl){
         theplayer.attr('src',audiourl);
         theplayer[0].play();
     });
});



