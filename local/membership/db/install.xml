<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/membership/db" VERSION="20240720" COMMENT="XMLDB file for Moodle local/membership"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
    >
    <TABLES>
        <TABLE NAME="local_subscriptions" COMMENT="Table for holding all data about subscriptions">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="sub_id" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_reference" TYPE="char" LENGTH="100" NOTNULL="true"/>
                <FIELD NAME="sub_user" TYPE="int" LENGTH="10" NOTNULL="false"/>
                <FIELD NAME="sub_email" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_platform" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_method" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_cohorts" TYPE="text" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_new_cohorts" TYPE="text" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="sub_cron" TYPE="int" LENGTH="10" NOTNULL="false"/>
                <FIELD NAME="sub_history" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="sub_status" TYPE="int" LENGTH="10" NOTNULL="false"/>
                <FIELD NAME="sub_modified" TYPE="int" LENGTH="10" NOTNULL="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
